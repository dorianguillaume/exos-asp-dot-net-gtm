@model IEnumerable<Ciqual.Models.Famille>

@{
    ViewData["Title"] = "Index";
}

<h1>Familles</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdFamille)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdFamille)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
        </tr>
}
    </tbody>
</table>
